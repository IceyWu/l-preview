{
  "name": "l-preview",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/l-preview.umd.js",
  "module": "./dist/l-preview.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
